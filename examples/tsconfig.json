
{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
